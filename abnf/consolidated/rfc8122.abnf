token = 1*(rfc4566-TOKEN-CHAR)
attribute = (rfc4566-ATT-FIELD ":" rfc4566-ATT-VALUE) / rfc4566-ATT-FIELD
rfc4566-TOKEN-CHAR = %d33 / %x23-27 / %x2a-2b / %x2d-2e / %x30-39 / %x41-5a / %x5e-7e
rfc4566-ATT-FIELD = token
rfc4566-ATT-VALUE = rfc4566-BYTE-STRING
rfc4566-BYTE-STRING = 1*(%x1-9 / %xb-c / %xe-ff)
